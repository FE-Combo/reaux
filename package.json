{
  "name": "core-react",
  "version": "1.0.0",
  "description": "Front-End lightweight framework base on react + redux + axios, strict in TypeScript.",
  "scripts": {
    "output": "yarn && node script/output.react.js && node script/upgrade-rn.js && rm -rf node_modules && yarn && node script/output.rn.js",
    "format": "prettier --config config/prettier.json --write \"{package,test}/**/*.{ts,tsx}\"",
    "upgrade-react": "node script/upgrade-react.js",
    "upgrade-rn": "node script/upgrade-rn.js",
    "output-react": "node script/output.react.js",
    "output-rn": "node script/output.rn.js",
    "publish-react": "rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vocoWone/core-react.git"
  },
  "keywords": [
    "javascript",
    "typescript"
  ],
  "author": "vocoWone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vocoWone/core-react/issues"
  },
  "homepage": "https://github.com/vocoWone/core-react#readme",
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-decorators": "7.2.0",
    "@babel/runtime": "7.4.3",
    "@types/react-native-snap-carousel": "3.7.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "fs-extra": "7.0.1",
    "metro-react-native-babel-preset": "0.53.1",
    "prettier": "1.16.4",
    "tslint": "5.15.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.3",
    "yargs": "13.2.2"
  },
  "dependencies": {
    "@types/node": "11.13.0",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.3",
    "@types/react-redux": "7.0.1",
    "@types/react-navigation": "3.0.6",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-saga": "1.0.2"
  }
}
